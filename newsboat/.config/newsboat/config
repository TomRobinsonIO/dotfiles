freshrss-url "https://freshrss.tomrobinson.io/api/greader.php"
freshrss-login "tom"
freshrss-passwordeval "pass Homeserver/freshrss"

#Feed Settings

refresh-on-startup yes
ignore-mode "download"

# Set Browser
# browser "brave"

# Unbind Defaults

unbind-key j
unbind-key k
unbind-key h
unbind-key H
unbind-key L
unbind-key g
unbind-key G

# Bind keys
bind-key h quit
bind-key j down
bind-key k up
bind-key l open
bind-key H next-feed
bind-key L prev-feed
bind-key u pageup
bind-key d pagedown
bind-key g home
bind-key G end
bind-key z sort
bind-key Z rev-sort
freshrss-flag-star "s"
